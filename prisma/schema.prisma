generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Books {
  Id            Int    @id(map: "PK_Books") @default(autoincrement())
  Title         String @db.NVarChar(Max)
  Author        String @db.NVarChar(Max)
  YearPublished Int
}

model EFMigrationsHistory {
  MigrationId    String @id(map: "PK___EFMigrationsHistory") @db.NVarChar(150)
  ProductVersion String @db.NVarChar(32)

  @@map("__EFMigrationsHistory")
}
